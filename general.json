{
  "extends": ["config:base"],
  "labels": ["dependency"],
  "unicodeEmoji": true,
  "semanticCommitType": "deps",
  "semanticCommits": "enabled",
  "ignoreDeprecated": true,
  "ignoreNpmrcFile": true,
  "includeForks": false,
  "prCreation": "not-pending",
  "prFooter": "This PR has been generated by renovateBot",
  "draftPR": false,
  "branchPrefix": "deps/",
  "additionalBranchPrefix": "",
  "commitBody": "Generated by renovateBot",
  "commitBodyTable": true,
  "schedule": ["every weekend"],
  "timezone": "Asia/Hong_Kong",
  "dependencyDashboard": true,
  "dependencyDashboardTitle": "Available dependency upgrades",
  "dependencyDashboardFooter": "- [ ] <!-- manual job -->Check this box to trigger a request for Renovate to run again on this repository",
  "dependencyDashboardApproval": true,
  "dependencyDashboardAutoclose": false,
  "prBodyDefinitions": {
    "Package": "`{{{lookupName}}}`"
  },
  "prBodyColumns": [
    "Package",
    "Update",
    "Type",
    "New value",
    "Package file",
    "References"
  ]
}
